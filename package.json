{
  "name": "ide-dlang",
  "main": "./lib/ide-dlang",
  "version": "1.4.0",
  "description": "D language support for Atom-IDE",
  "keywords": [
    "dlang",
    "d",
    "atom-ide",
    "ide",
    "ide-d"
  ],
  "repository": "https://github.com/LaurentTreguier/ide-dlang",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "activationHooks": [
    "ide-dlang:grammar-used"
  ],
  "enhancedScopes": [
    "source.d"
  ],
  "configSchema": {
    "dlsPath": {
      "title": "Path to the dls executable. Will be updated automatically, do not change.",
      "type": "string",
      "default": ""
    },
    "dls": {
      "title": "DLS configuration",
      "type": "object",
      "properties": {
        "symbol": {
          "title": "Symbols",
          "type": "object",
          "properties": {
            "importPaths": {
              "title": "Import paths",
              "type": "array",
              "default": []
            }
          }
        },
        "analysis": {
          "title": "Analysis",
          "type": "object",
          "properties": {
            "configFile": {
              "title": "D-Scanner config file",
              "type": "string",
              "default": "dscanner.ini"
            }
          }
        },
        "format": {
          "title": "Formatting",
          "type": "object",
          "properties": {
            "endOfLine": {
              "title": "End of line",
              "type": "string",
              "enum": [
                "lf",
                "cr",
                "crlf"
              ],
              "default": "lf"
            },
            "maxLineLength": {
              "title": "Max line length",
              "type": "number",
              "default": 120
            },
            "dfmtAlignSwitchStatements": {
              "title": "Align switch statements",
              "type": "boolean",
              "default": true
            },
            "dfmtBraceStyle": {
              "title": "Brace style",
              "type": "string",
              "enum": [
                "allman",
                "otbs",
                "stroustrup"
              ],
              "default": "allman"
            },
            "dfmtOutdentAttributes": {
              "title": "Outdent attributes",
              "type": "boolean",
              "default": true
            },
            "dfmtSoftMaxLineLength": {
              "title": "Soft max line length",
              "type": "number",
              "default": 80
            },
            "dfmtSpaceAfterCast": {
              "title": "Space after cast",
              "type": "boolean",
              "default": true
            },
            "dfmtSpaceAfterKeywords": {
              "title": "Space after keywords",
              "type": "boolean",
              "default": true
            },
            "dfmtSpaceBeforeFunctionParameters": {
              "title": "Space before function parameters",
              "type": "boolean",
              "default": false
            },
            "dfmtSplitOperatorAtLineEnd": {
              "title": "Split operator at line end",
              "type": "boolean",
              "default": false
            },
            "dfmtSelectiveImportSpace": {
              "title": "Selective import space",
              "type": "boolean",
              "default": true
            },
            "dfmtCompactLabeledStatements": {
              "title": "Compact labeled statements",
              "type": "boolean",
              "default": true
            },
            "dfmtTemplateConstraintStyle": {
              "title": "Template Constraint Style",
              "type": "string",
              "enum": [
                "conditionalNewlineIndent",
                "conditionalNewline",
                "alwaysNewline",
                "alwaysNewlineIndent"
              ],
              "default": "conditionalNewlineIndent"
            },
            "dfmtSingleTemplateConstraintIndent": {
              "title": "Single Template Constraint Indent",
              "type": "boolean",
              "default": false
            }
          }
        }
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "dependencies": {
    "atom-languageclient": "^0.9.5"
  },
  "devDependencies": {}
}
