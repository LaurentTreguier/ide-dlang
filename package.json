{
  "name": "ide-dlang",
  "main": "./lib/ide-dlang",
  "version": "1.11.2",
  "description": "D language support for Atom-IDE",
  "keywords": [
    "dlang",
    "d",
    "atom-ide",
    "ide",
    "ide-d"
  ],
  "repository": "https://github.com/d-language-server/atom-ide-dlang",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "activationHooks": [
    "ide-dlang:grammar-used"
  ],
  "enhancedScopes": [
    "source.d",
    "source.dpp"
  ],
  "configSchema": {
    "dubPath": {
      "title": "Path to the dub executable",
      "type": "string",
      "default": "dub"
    },
    "dlsPath": {
      "title": "Path to the dls executable",
      "type": "string",
      "default": ""
    },
    "init": {
      "title": "DLS initatization options",
      "type": "object",
      "properties": {
        "autoUpdate": {
          "title": "Enable auto-updating",
          "type": "boolean",
          "default": true
        },
        "catchErrors": {
          "title": "Catch and ignore errors (WARNING: UNSAFE)",
          "type": "boolean",
          "default": false
        },
        "capabilities": {
          "title": "DLS enabled capabilities",
          "type": "object",
          "properties": {
            "hover": {
              "title": "Enable hover",
              "type": "boolean",
              "default": true
            },
            "completion": {
              "title": "Enable completion",
              "type": "boolean",
              "default": true
            },
            "definition": {
              "title": "Enable go-to-definition",
              "type": "boolean",
              "default": true
            },
            "typeDefinition": {
              "title": "Enable go-to-type-definition",
              "type": "boolean",
              "default": true
            },
            "references": {
              "title": "Enable references search",
              "type": "boolean",
              "default": true
            },
            "documentHighlight": {
              "title": "Enable symbol highlighting",
              "type": "boolean",
              "default": true
            },
            "documentSymbol": {
              "title": "Enable document symbol search",
              "type": "boolean",
              "default": true
            },
            "workspaceSymbol": {
              "title": "Enable workspace symbol search",
              "type": "boolean",
              "default": true
            },
            "codeAction": {
              "title": "Enable code actions",
              "type": "boolean",
              "default": true
            },
            "documentFormatting": {
              "title": "Enable formatting",
              "type": "boolean",
              "default": true
            },
            "documentRangeFormatting": {
              "title": "Enable range formatting",
              "type": "boolean",
              "default": true
            },
            "documentOnTypeFormatting": {
              "title": "Enable on type formatting",
              "type": "boolean",
              "default": true
            },
            "rename": {
              "title": "Enable renaming",
              "type": "boolean",
              "default": true
            }
          }
        }
      }
    },
    "dls": {
      "title": "DLS configuration",
      "type": "object",
      "properties": {
        "symbol": {
          "title": "Symbols",
          "type": "object",
          "properties": {
            "importPaths": {
              "title": "Import paths",
              "type": "array",
              "default": []
            }
          }
        },
        "analysis": {
          "title": "Analysis",
          "type": "object",
          "properties": {
            "configFile": {
              "title": "D-Scanner config file",
              "type": "string",
              "default": "dscanner.ini"
            }
          }
        },
        "format": {
          "title": "Formatting",
          "type": "object",
          "properties": {
            "endOfLine": {
              "title": "End of line",
              "type": "string",
              "enum": [
                "lf",
                "cr",
                "crlf"
              ],
              "default": "lf"
            },
            "maxLineLength": {
              "title": "Max line length",
              "type": "number",
              "default": 120
            },
            "dfmtAlignSwitchStatements": {
              "title": "Align switch statements",
              "type": "boolean",
              "default": true
            },
            "dfmtBraceStyle": {
              "title": "Brace style",
              "type": "string",
              "enum": [
                "allman",
                "otbs",
                "stroustrup"
              ],
              "default": "allman"
            },
            "dfmtOutdentAttributes": {
              "title": "Outdent attributes",
              "type": "boolean",
              "default": true
            },
            "dfmtSoftMaxLineLength": {
              "title": "Soft max line length",
              "type": "number",
              "default": 80
            },
            "dfmtSpaceAfterCast": {
              "title": "Space after cast",
              "type": "boolean",
              "default": true
            },
            "dfmtSpaceAfterKeywords": {
              "title": "Space after keywords",
              "type": "boolean",
              "default": true
            },
            "dfmtSpaceBeforeFunctionParameters": {
              "title": "Space before function parameters",
              "type": "boolean",
              "default": false
            },
            "dfmtSplitOperatorAtLineEnd": {
              "title": "Split operator at line end",
              "type": "boolean",
              "default": false
            },
            "dfmtSelectiveImportSpace": {
              "title": "Selective import space",
              "type": "boolean",
              "default": true
            },
            "dfmtCompactLabeledStatements": {
              "title": "Compact labeled statements",
              "type": "boolean",
              "default": true
            },
            "dfmtTemplateConstraintStyle": {
              "title": "Template constraint style",
              "type": "string",
              "enum": [
                "conditionalNewlineIndent",
                "conditionalNewline",
                "alwaysNewline",
                "alwaysNewlineIndent"
              ],
              "default": "conditionalNewlineIndent"
            },
            "dfmtSingleTemplateConstraintIndent": {
              "title": "Single template constraint indent",
              "type": "boolean",
              "default": false
            }
          }
        }
      }
    }
  },
  "consumedServices": {
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "builder": {
      "versions": {
        "2.0.0": "provideBuild"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.onSave": {
      "versions": {
        "0.1.0": "provideOnSaveCodeFormat"
      }
    },
    "code-format.onType": {
      "versions": {
        "0.1.0": "provideOnTypeCodeFormat"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "package-deps": [
    "atom-ide-ui",
    "build"
  ],
  "dependencies": {
    "atom-ide-ui": "^0.7.2",
    "atom-languageclient": "^0.9.7",
    "bytes": "^3.0.0",
    "glob": "^7.1.3"
  },
  "devDependencies": {}
}
